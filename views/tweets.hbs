<script type="text/javascript" src="/socket.io/socket.io.js"></script>

<h1>{{title}}</h1>
{{#each tweets}}
    <li>{{this.tweet}}</li>
{{/each}}

<script data-main="javascripts/main" src="javascripts/require.js"></script>

<script id="tweet-template" type="text/x-handlebars-template">
    <div class="tweet">
        <h1>{{title}}</h1>
        <div class="body">
            {{body}}
        </div>
    </div>
</script>